import java.util.Scanner;

public class primeira_fase_exercicio_for7 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		if (n >= 0) {
		
			for (int i=1; i<=n; i++) {
				
				System.out.println(i);
				
			}
		} else {
			
			System.out.println("Número inválido");
		}
	}

}
