package entities;

public class Rectangle {

	public static void main(String[] args) {

		public double width;
		public double height;
	
		public double Area() {
			
			return width * height;
		}
		
		public double Perimeter() {
			
			return (width * 2) + (height * 2);
		}
		
		public double Diagonal() {
			
			return Math.pow(width, height);
			
		}
		
	}
}
