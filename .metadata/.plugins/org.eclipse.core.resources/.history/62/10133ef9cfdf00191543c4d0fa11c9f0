import java.util.Scanner;

public class primeira_fase_exercicio_for6 {
	
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int divisor = 0;
		
		for (int i=0; i<=n; i++) {
			
			if ((n % i) == 0) {
				
				divisor = n;
				
				System.out.println(divisor);
			}
		}
		
		sc.close();
		
	}

}
