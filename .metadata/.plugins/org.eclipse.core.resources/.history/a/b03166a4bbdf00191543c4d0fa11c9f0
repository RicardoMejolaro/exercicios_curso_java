import java.util.Scanner;

public class primeira_fase_exercicio_for1 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		
		if (x >= 1 && x <= 1000) {
		
			for (int i = 1; i < x; i++) {
			
				if ((x % 2) != 0) {
				
					System.out.println(x);
				
				}
			
			}
		
		}	
	}

}
